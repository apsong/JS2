							<Name>TransientDocument</Name>
							<Value>{dashdocument}</Value>
						</ParameterValue>
						<ParameterValue>
							<Name>SaveFile</Name>
							<Value>false</Value>
						</ParameterValue>
					</ParameterValues>
				</Data>
			</Operation>
		</GetDashboardUpdatedObjects>
	</soapenv:Body>
</soapenv:Envelope>			
							]]>
						</postbody>
						
						<httpresponse>
							<read body="&lt;Json>(.+?)&lt;/Json>" into="jsondata"/>
							<read body="&lt;DashboardSessionID>(.+?)&lt;/DashboardSessionID>" into="dashsession"/>
							
						</httpresponse>
						
					</httppost>
					<json var="jsondata" read="status" into="generationStatus"/>
						
					<set var="getGenStatusCount" value="({getGenStatusCount} + 1)"/>
					<if cond="({getGenStatusCount} GE {GenStatusLimit})">
						<verify cond="false" name="GetGenerationStatus took longer than {GenStatusLimit} seconds" onfail="break"/>
