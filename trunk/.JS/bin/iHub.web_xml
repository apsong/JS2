#! /usr/bin/sh

for WEB_XML in `find -regex ".*/iportal/.*/web.xml"`; do
    [ ! -f $WEB_XML.orig ] && cp -v $WEB_XML $WEB_XML.orig
    sed -i -e 's/\<20\>/300/' $WEB_XML
    unix2dos $WEB_XML
    diff $WEB_XML.orig $WEB_XML
done

mv -v register.bat __register.bat
mv -v unregister.bat __unregister.bat
mv -v setupiHub.bat __setupiHub.bat
